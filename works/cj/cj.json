  {
    "command": "ChapterTitle",
    "arg1": "<span style=\"text-shadow:0 0 12px #00000085\">假如满穗加入了闯军……</span>\n<span style=\"text-shadow:0 0 12px #00000085;transform:scale(0.5)\">莫莫莫里哀</span>\n<span style=\"transform: scale(0.5);text-shadow:0 0 12px #00000085\">www.bilibili.com/video/BV1om411y7UN</span>"
  },
